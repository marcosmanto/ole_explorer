<h1>OleExplorer</h1>
<%= select "typelib", "typelib", @typelibs, {:prompt=>"Select Type Library"}  %>

<p id="ole_classes">
  <%= render @ole_classes %>
</p>

<p id="ole_members">
  <%= render :partial => 'ole_members' %>
</p>

<p id="member_info">
  <%= render :partial => 'member_info', :object => @member %>
</p>